@model AinShamsHospital.ViewModels.delete

@{
    ViewData["Title"] = "Cancle";
    Layout = "~/Views/Front_Layout.cshtml";

}


<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Cancel">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="PatientName" class="control-label"></label>
                <input asp-for="PatientName" class="form-control" />
                <span asp-validation-for="PatientName" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Cancel Reservation" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>
<div>
    <div class="text-success">@ViewBag.UserMessage4</div>
    <div class="text-danger">@ViewBag.UserMessage5</div>
   
</div>


@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
