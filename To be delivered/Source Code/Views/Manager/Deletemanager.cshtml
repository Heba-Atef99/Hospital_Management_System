@model AinShamsHospital.ViewModels.deletvm

@{
    ViewData["Title"] = "Deletemanager";
    Layout = "~/Views/Manager_Layout.cshtml";
}

<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Deletemanager">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="StaffName" class="control-label"></label>
                <input asp-for="StaffName" class="form-control" />
                <span asp-validation-for="StaffName" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Delete Staff Member" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>
<div class="text-success">@ViewBag.UserMessage4</div>
<div class="text-danger">@ViewBag.UserMessage5</div>
