@{
}
<section class="container">
    <form asp-action="stateview">
        <h2></h2>
        <hr />
        <div class="row">

            @foreach (var fp in ViewBag.type)
            {
                <div class="card col-md-3 mx-2 mb-3 p-0">
                    <div class="card-body">
                        <h4 class="card-title text-danger"> @fp.Name </h4>
                        <button name="Room_Id" value="@fp.Name" type="submit" class="stretched-link"></button>
                    </div>
                </div>
            }
        </div>

    </form>
</section>