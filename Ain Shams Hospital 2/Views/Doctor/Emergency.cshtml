@page
@{
    var Blood_Unit = ViewBag.Blood_Units;
}
@{
    var facility = ViewBag.Hospital_Facility;
}
<h1>Blood_Unit</h1>
<br />
<div class="row">
    @foreach (var p in Blood_Unit)
    {

        <div class="card card-body">

            <h1>Type :@p.Type</h1>
            <hr />


            <h3>Amount :</h3>
            <h3>@p.Amount</h3>
            <hr />

        </div>

    }
</div>
<h1>Emergency bed</h1>
<br />
<div class="row">
    @foreach (var n in facility)
    {

        <div class="card card-body">

            <h1>Type :@n.Type</h1>
            <hr />
            <h3>Available :</h3>
            <h3>@n.Available</h3>
            <hr />
            <h3> Start_Working :</h3>
            <h3>@n.Start_Working_Hour</h3>
            <hr />
            <h3>End_Working :</h3>
            <h3>@n.End_Working_Hour</h3>
            <hr />

        </div>

    }
</div>